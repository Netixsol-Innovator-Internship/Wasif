{
  "version": 2,
  "buildCommand": "npm run build",
  "outputDirectory": ".",
  "headers": [
    {
      "source": "/(.*\\.js)$",
      "headers": [
        {
          "key": "Content-Type",
          "value": "application/javascript"
        }
      ]
    }
  ],
  "rewrites": [
    {
      "source": "/((?!output\\.css|assets/|script\\.js|.*\\.js).*)",
      "destination": "/index.html"
    }
  ]
}